<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="metadata.disneyinfo.nl"
       name="Disney Video NL"
       version="1.0.1"
       provider-name="Magnetism">
  <requires>
    <import addon="xbmc.metadata" version="1.0"/>
    <import addon="metadata.common.themoviedb.org" version="1.0"/>
  </requires>
  <extension point="xbmc.metadata.scraper.movies"
             language="nl"
             library="disneyinfo.xml"/>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">DisneyInfo Movie Scraper</summary>
    <summary lang="hu">DisneyInfo filmadat leolvasó</summary>
    <summary lang="kr">DisneyInfo 영화 스크래퍼</summary>
    <summary lang="pl">Scraper filmów DisneyInfo</summary>
    <summary lang="pt">Scraper de filmes DisneyInfo</summary>
    <description lang="en">Download Movie information from www.disneyinfo.nl</description>
    <description lang="hu">Film információk letöltése a www.disneyinfo.nl webhelyről</description>
    <description lang="kr">www.disneyinfo.nl 에서 영화 정보 다운로드</description>
    <description lang="pl">Pobieraj informacje o filmach z www.disneyinfo.nl</description>
    <description lang="pt">Descarregar informação de filmes de www.disneyinfo.nl</description>
    <platform>all</platform>
  </extension>
</addon>
